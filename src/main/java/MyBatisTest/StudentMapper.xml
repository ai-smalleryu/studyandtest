<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MyBatisTest.StudentMapperInter">
    <resultMap id="sysUser" type="MyBatisTest.Student1">
        <id property="id" column="id"></id>
        <result property="name" column="name"></result>
        <result property="password" column="password"></result>
        <result property="age" column="age"></result>
    </resultMap><!--resultType="student1" databaseId="mysql" -->
    <select id="getStudentId" resultMap="sysUser"> /*resultType 你的返回值类型，可以在mybatisconfig类进行设置*/
    select * from student1 where id = #{id}  </select>
</mapper>

